{
  "mcpServers": {
    "n8n": {
      "type": "http",
      "description": "n8n workflow automation integration",
      "baseUrl": "${N8N_API_URL}",
      "authentication": {
        "type": "api-key",
        "header": "X-N8N-API-KEY",
        "value": "${N8N_API_KEY}"
      },
      "endpoints": {
        "workflows": "/api/v1/workflows",
        "executions": "/api/v1/executions",
        "credentials": "/api/v1/credentials"
      },
      "enabled": true
    },
    "supabase": {
      "type": "postgrest",
      "description": "Supabase backend integration",
      "baseUrl": "${SUPABASE_URL}",
      "authentication": {
        "type": "bearer",
        "header": "apikey",
        "value": "${SUPABASE_ANON_KEY}"
      },
      "endpoints": {
        "rest": "/rest/v1",
        "auth": "/auth/v1",
        "storage": "/storage/v1"
      },
      "enabled": true
    }
  },
  "version": "1.0.0",
  "created": "2025-11-11"
}
